					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Ottawa"
					android:id="@+id/rdBtn23"
					android:textSize="24px" />
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Toront"
					android:id="@+id/rdBtn24"
					android:textSize="24px" />
			</RadioGroup>
		</LinearLayout>
		<LinearLayout
			android:orientation="vertical"
			android:minWidth="25px"
			android:minHeight="25px"
			android:layout_width="match_parent"
			android:layout_height="64.5dp"
			android:id="@+id/subLinearLayout2">
			<TextView
				android:text="3. Hi"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_width="290dp"
				android:layout_height="wrap_content"
				android:id="@+id/ques3TV"
				android:textSize="32px" />
			<RadioGroup
				android:minWidth="25px"
				android:minHeight="25px"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal"
				android:id="@+id/rdBtn3">
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Vancouver"
					android:id="@+id/rdBtn31"
					android:textSize="24px" />
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Montreal"
					android:id="@+id/rdBtn32"
					android:textSize="24px" />
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Ottawa"
					android:id="@+id/rdBtn33"
					android:textSize="24px" />
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Toront"
					android:id="@+id/rdBtn34"
					android:textSize="24px" />
			</RadioGroup>
		</LinearLayout>
		<LinearLayout
			android:orientation="vertical"
			android:minWidth="25px"
			android:minHeight="25px"
			android:layout_width="match_parent"
			android:layout_height="64.5dp"
			android:id="@+id/subLinearLayout2"
			android:layout_marginBottom="0.0dp">
			<TextView
				android:text="4. Hi"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_width="290dp"
				android:layout_height="wrap_content"
				android:id="@+id/ques4TV"
				android:textSize="32px" />
			<RadioGroup
				android:minWidth="25px"
				android:minHeight="25px"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal"
				android:id="@+id/rdBtn4">
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Vancouver"
					android:id="@+id/rdBtn41"
					android:textSize="24px" />
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Montreal"
					android:id="@+id/rdBtn42"
					android:textSize="24px" />
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Ottawa"
					android:id="@+id/rdBtn43"
					android:textSize="24px" />
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Toront"
					android:id="@+id/rdBtn44"
					android:textSize="24px" />
			</RadioGroup>
		</LinearLayout>
		<LinearLayout
			android:orientation="vertical"
			android:minWidth="25px"
			android:minHeight="25px"
			android:layout_width="match_parent"
			android:layout_height="90dp"
			android:id="@+id/subLinearLayout2">
			<TextView
				android:text="5. Hi"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_width="290dp"
				android:layout_height="wrap_content"
				android:id="@+id/ques5TV"
				android:textSize="32px" />
			<RadioGroup
				android:minWidth="25px"
				android:minHeight="25px"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal"
				android:id="@+id/rdBtn5">
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Vancouver"
					android:id="@+id/rdBtn51"
					android:textSize="24px" />
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Montreal"
					android:id="@+id/rdBtn52"
					android:textSize="24px" />
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Ottawa"
					android:id="@+id/rdBtn53"
					android:textSize="24px" />
				<RadioButton
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Toront"
					android:id="@+id/rdBtn54"
					android:textSize="24px" />
			</RadioGroup>
		</LinearLayout>
		<TextView
			android:text="Total Score : "
			android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/scoreTv"
			android:layout_marginTop="-55px"
			android:textSize="43px" />
	</LinearLayout>

</android.support.design.widget.CoordinatorLayout>
